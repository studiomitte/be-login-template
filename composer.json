{
    "name": "studiomitte/be-login-template",
    "type": "typo3-cms-extension",
    "description": "Override template of backend login",
    "authors": [
        {
            "name": "Georg Ringer",
            "role": "Developer"
        }
    ],
    "homepage": "https://studiomitte.com",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^11"
    },
    "autoload": {
        "psr-4": {
            "StudioMitte\\BackendLogin\\": "Classes"
        }
    },
    "replace": {
        "studiomitte/be-login-template": "self.version",
        "typo3-ter/be-login-template": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "be_login_template"
        }
    }
}
